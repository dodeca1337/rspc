{
  "name": "rspc-msgpack-ws-transport",
  "version": "1.0.0",
  "author": "Oscar Beaumont",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && tsc",
    "prepare": "tsup && tsc"
  },
  "dependencies": {
    "@msgpack/msgpack": "2.8.0",
    "@rspc/client": "0.2.4"
  },
  "devDependencies": {
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "splitting": true,
    "clean": true,
    "sourcemap": true
  }
}